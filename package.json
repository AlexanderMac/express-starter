{
  "name": "express-starter",
  "version": "0.0.1",
  "description": "Express-starter",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "scripts": {
    "start": "nodemon --config .nodemon.json ./src/app.js",
    "lint": "eslint -f stylish 'src/**/*.js' 'test/**/*.js'",
    "test": "NODE_ENV=test mocha",
    "coverage": "nyc npm test",
    "coverage-gh": "nyc --reporter=lcov npm test && codecov"
