{
  "name": "express-starter",
  "version": "0.0.1",
  "description": "Express-starter",
  "private": true,
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "ts-node-dev --rs --cls src/index.ts",
    "clean": "rm -rf ./build",
    "build": "pnpm clean && pnpm tsc",
    "lint": "eslint ./src ./test --ext .ts --fix",
    "prettify": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format": "pnpm lint && pnpm prettify",
    "test": "NODE_ENV=test mocha",
    "test-ci": "NODE_ENV=test-ci mocha",
    "coverage": "nyc npm test",
    "coverage-gh": "nyc --reporter=lcov npm test && codecov"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.3.0",
    "morgan": "^1.10.0",
    "n-params-processor": "^6.0.0",
    "nconf": "^0.12.0",
    "pug": "^3.0.2",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/nconf": "^0.10.3",
    "@types/node": "^20.3.1",
    "@types/sinon": "^10.0.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mocha": "^10.2.0",
    "n-assert": "^7.0.0",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "sinon": "^15.1.2",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexanderMac/express-starter"
  },
  "author": "Alexander Mac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexanderMac/express-starter/issues"
  },
  "homepage": "https://github.com/AlexanderMac/express-starter"
}
