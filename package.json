    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "lodash-es": "^4.17.21",
    "mongoose": "^5.13.14",
    "morgan": "^1.10.0",
    "n-params-processor": "^6.0.0",
    "nconf": "^0.12.0",
    "pug": "^3.0.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "eslint": "^8.21.0",
    "mocha": "^10.0.0",
    "n-assert": "^7.0.0",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "sinon": "^14.0.0",
    "supertest": "^6.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexanderMac/express-starter"
